<UserControl x:Class="WowConfigCopy.UI.Views.AccountDetails"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:WowConfigCopy.UI.Views"
             mc:Ignorable="d">
    <Grid>
        <TextBlock Text="{Binding AccountName}" FontSize="24" Foreground="{StaticResource TextBrush}" Margin="5" FontFamily="{StaticResource ZenKakuGothicAntiqueBold}"/>
    </Grid>
</UserControl>
